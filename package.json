    "name": "Dima Gurkalenko",
    "email": "gurkalenko.d@gmail.com"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-chat-elements": "^10.8.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "ts-md5": "^1.2.7",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "prettier": "prettier --write {src,cypress}/**/*.{js,ts,tsx}",
    "test:once": "npm test -- --watchAll=false",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "typecheck": "tsc --noEmit",
    "check:modules": "tsc --traceResolution",
    "cov": "npm test -- --coverage",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:chrome:headless": "cypress run --browser chrome --headless",
    "cypress:open": "cypress open",
    "cypress:run:percy": "percy exec -- cypress run --record",
    "ci:start_server_and_percy": "percy exec -- yarn start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:once && npm run typecheck"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.2.0",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.4",
    "redux-devtools-extension": "^2.13.8"
  }
